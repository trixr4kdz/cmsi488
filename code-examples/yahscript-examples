//Prime Function

prime is (n) ->
    for i in (0...n/2):
        if eq n % i, 0:
            spit nah
        i++
    spit yah



//displaying use of our equals operator that can do multiple comparisons


//declaring and assigning to four variables
x is 1
y is 2 - 1
z is 3 - 2
u is 4 - 3

//if statement and equals operator
if eq x,y,z,u:
    spit yah
else:
    spit nah



if (eq x,y,z,u,meh or x > -3):          // We can have blocks for doing multiple things in
    spit yah 
else:
    meh is "wat"
    spit nah


//string concatenation

stringOne is "sup"
stringTwo is "bruh"
banana is 10

stringThree is "sup \(StringTwo) here's \(banana) dollars"

print(stringThree)
yaw(stringThree)


// this would print "sup bruh here's 10 dollars"


//relational operators

gt 10,11,11,1,1,1,1
lt 10,12,1,1,1,1,1
geq 11,11,1,1,1,1,1
leq 12,12,1,1,1,1,1
eq 10,10